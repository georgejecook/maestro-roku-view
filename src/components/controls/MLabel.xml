<?xml version="1.0" encoding="utf-8" ?>
<component
    name="MLabel"
    extends="Label">
  <interface>
    <!-- convenience wrapper for all style types, so they can
    easily be passed in, from an AA -->
    <field
        id="style"
        type="assocarray" />

    <field
        id="systemFont"
        type="string"
        value="Smallest, 21" />


  </interface>
</component>